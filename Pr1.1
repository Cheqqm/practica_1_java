package Practic1;
import java.util.Scanner;
public class Class1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] b = new int[10];
        for (int i = 0; i<10 ; i++){
            b[i] = sc.nextInt();
        }
        int summa = 0;
        for (int i = 0; i<10 ; i++){
            summa += b[i];
        }
        int srd = 0;
        srd = summa / 10;
        System.out.print(summa + "\n"+ srd + " ");
        System.out.println(srd);
    }

}
